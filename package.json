{
  "name": "react-basic-router",
  "version": "1.0.4",
  "description": "A basic react router that uses hash state for navigation.",
  "main": "index.js",
  "scripts": {
    "build": "gulp default",
    "prepublish": "npm run build; npm run test",
    "test": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --require testfile.jsx",
    "cover": "cross-env NODE_ENV=test nyc --reporter=lcov --reporter=text mocha --require testfile.jsx",
    "coveralls": "cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "keywords": [
    "react",
    "router",
    "routing",
    "hash"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/SharpCoder/react-basic-router.git"
  },
  "nyc": {
    "require": [
      "babel-register"
    ],
    "sourceMap": false,
    "instrument": false
  },
  "author": "sharpcoder",
  "license": "MIT",
  "dependencies": {
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-istanbul": "^4.1.3",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "chai": "^3.5.0",
    "coveralls": "^2.13.1",
    "cross-env": "^4.0.0",
    "del": "^2.2.2",
    "enzyme": "^3.0.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "gulp": "^3.9.1",
    "gulp-babel": "^6.1.2",
    "gulp-concat": "^2.6.1",
    "istanbul": "^0.4.5",
    "jsdomify": "^3.1.0",
    "mocha": "^3.3.0",
    "mocha-lcov-reporter": "^1.3.0",
    "nyc": "^10.3.2",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-test-renderer": "^16.2.0"
  }
}
